<!DOCTYPE html>
<html>
<head>
	<title>Delete Customer</title>
	<link rel="stylesheet" href="/assets/common/css/style.css">
</head>
<body>
	<%- include('NavBar') %>
	<div id="workspace">
		<form method="post">
		<fieldset>
			<legend>Delete Customer</legend>
			<table>
				<tr>
					<td>Profile Picture</td>
					<td><img src="<%=list[0].profilepicture%>" alt="Avatar" width="100" height="100"></td>
				</tr>
				<tr>
					<td>Customer Id :</td>
					<td><%=list[0].customerid %></td>
				</tr>
				<tr>
					<td>Name :</td>
					<td><%=list[0].name %></td>
				</tr>
				<tr>
					<td>Email :</td>
					<td><%=list[0].email %></td>
				</tr>
				<tr>
					<td>Gender :</td>
					<td><%=list[0].gender %></td>
				</tr>
				<tr>
					<td>Date Of Birth :</td>
					<td><%=list[0].dob %></td>
				</tr>
				<tr>
					<td>Phone Number :</td>
					<td><%=list[0].phonenumber %></td>
				</tr>
				<tr>
					<td>Address :</td>
					<td><%=list[0].address %></td>
				</tr>
				<tr>
					<td>Status :</td>
					<td><%=list[0].status %></td>
				</tr>
			</table>
			<div>
				<h3>Are you sure?</h3>
				<a href="/adminController/customerList">
					<button type="button">
						Back
					</button>
				</a>
				<input type="submit" name="submit" value="Delete">
			</div>
		</fieldset>
	</form>
	</div>
</body>
</html>